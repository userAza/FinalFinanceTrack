<Window x:Class="FinalFinanceTrack.UserMan"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="User Management" Height="500" Width="800">
    <Grid Background="#FFF9F6F4">

        <Button x:Name="UserListButton" Content="USER LIST" HorizontalAlignment="Center" VerticalAlignment="Top" Width="192" Height="49" Margin="0,108,0,0" Click="UserListButton_Click"/>
        <Button x:Name="AddNewUserButton" Content="ADD NEW USER" HorizontalAlignment="Center" VerticalAlignment="Top" Width="192" Height="47" Margin="0,168,0,0" Click="AddNewUserButton_Click"/>
        <Button x:Name="EditUserButton" Content="EDIT USER" HorizontalAlignment="Center" VerticalAlignment="Top" Width="192" Height="43" Margin="0,228,0,0" Click="EditUserButton_Click"/>
        <Button x:Name="DeleteUserButton" Content="DELETE" HorizontalAlignment="Center" VerticalAlignment="Top" Width="192" Height="40" Margin="0,288,0,0" Click="DeleteUserButton_Click"/>

        <Button x:Name="BackButton" Content="Back" HorizontalAlignment="Left" VerticalAlignment="Top" Width="75" Height="30" Margin="10,10,0,0" Click="BackButton_Click"/>

    </Grid>
</Window>
