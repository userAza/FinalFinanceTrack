<Window x:Class="FinalFinanceTrack.MainPage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Main Page" Height="450" Width="1000" Background="White">
    <Grid>
        <Button x:Name="ReportsButton" Content="REPORTS" HorizontalAlignment="Left" VerticalAlignment="Top" Width="229" Height="53" Margin="50,341,0,0"
                Background="#D3D3D3" Foreground="Black" BorderBrush="#A9A9A9" Click="ReportsButton_Click"/>
        <Button x:Name="UserManagementButton" Content="USER MANAGEMENT" HorizontalAlignment="Left" VerticalAlignment="Top" Width="280" Height="53" Margin="550,341,0,0"
                Background="#D3D3D3" Foreground="Black" BorderBrush="#A9A9A9" Click="UserManagementButton_Click"/>
        <Button x:Name="OverviewButton" Content="OVERVIEW" HorizontalAlignment="Left" VerticalAlignment="Top" Width="93" Height="34" Margin="12,59,0,0"
                Background="#D3D3D3" Foreground="Black" BorderBrush="#A9A9A9" Click="OverviewButton_Click"/>
        <Button x:Name="SettingsButton" Content="SETTINGS" HorizontalAlignment="Left" VerticalAlignment="Top" Width="93" Height="34" Margin="771,22,0,0"
                Background="#D3D3D3" Foreground="Black" BorderBrush="#A9A9A9" Click="SettingsButton_Click"/>
    </Grid>
</Window>
