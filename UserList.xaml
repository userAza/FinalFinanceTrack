        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="User List" Height="450" Width="800">
    <Grid>
        <DataGrid x:Name="UserDataGrid" HorizontalAlignment="Left" Height="300" VerticalAlignment="Top" Width="780" Margin="10,10,0,0"/>
